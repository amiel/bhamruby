<% semantic_form_for(@company, :html => { :multipart => true }) do |f| %>
  
  <% f.inputs do %>
    <%= f.input :name %>
    <%= f.input :url %>
    <%= f.input :address %>
    <%= f.input :phone %>
    <%= f.input :owner %>
    <%= f.input :logo %>
  <% end %>
  
  
  <%= f.buttons :commit %>
<% end %>

